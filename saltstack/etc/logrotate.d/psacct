# Logrotate file for psacct RPM
 
/var/account/pacct {
#prerotate loses accounting records, let's no
#   prerotate
#       /usr/sbin/accton
#   endscript
    compress
    delaycompress
    notifempty
    daily
    rotate 31
    create 0600 root root
    postrotate
       /usr/sbin/accton /var/account/pacct
    endscript
}     
